{
  "name": "red-first",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.19.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext ts,tsx --no-cache",
    "lint:fix": "eslint . --ext ts,tsx --fix --no-cache",
    "format": "prettier --write .",
    "test:node": "jest tests --coverage --maxWorkers=10 --detectOpenHandles --config jest.node.config.cjs",
    "test:node:watch": "jest --coverage --watch --config --detectOpenHandles jest.node.config.cjs",
    "test:jsdom": "jest tests --coverage --maxWorkers=10 --detectOpenHandles --config ./jest.jsdom.config.cjs",
    "test:jsdom:watch": "jest --coverage --watch --detectOpenHandles --config ./jest.jsdom.config.cjs",
    "generate:migration:local": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "generate:migration:dev": "dotenv -e .env.dev -- npx prisma migrate dev --name init",
    "generate:migration:staging": "dotenv -e .env.staging -- npx prisma migrate dev --name init",
    "generate:migration:prod": "dotenv -e .env.prod -- npx prisma migrate dev --name init"
  },
